    "babel-loader": "^8.0.6",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "browser-sync": "2.26.x",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "deep-extend": "0.5.1",
    "del": "5.0.0",
    "file-loader": "^5.1.0",
    "gulp": "4.0.x",
    "gulp-babel": "8.0.0",
    "gulp-csso": "3.0.1",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-uglify": "3.0.2",
    "gulp-webpack": "1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "merge": "1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "stylelint": "10.1.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "terser-webpack-plugin": "^2.3.5",
    "uglifyjs-webpack-plugin": "2.2.0",
